<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <navigation-rule>
        <from-view-id>/documents/doc_income_list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>doc_income</from-outcome>
            <to-view-id>/documents/doc_income.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/documents/doc_income.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/documents/doc_income_list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <!--<from-view-id>/main.xhtml</from-view-id>-->
        <navigation-case>
            <from-outcome>doc_income_list</from-outcome>
            <to-view-id>/documents/doc_income_list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>doc_outcome_list</from-outcome>
            <to-view-id>/documents/doc_outcome_list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>doc_displacement_list</from-outcome>
            <to-view-id>/documents/doc_displacement_list.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
</faces-config>